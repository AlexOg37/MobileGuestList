
@section Header
{
	@Html.Partial("_HeaderBackButtonPartial")
}


<div>
      <div>
        <table id="winners" width="300" align="center" border="0" cellspacing="0" cellpadding="0">
          <tbody>
            <tr>
              <td height="5">
                &nbsp;
              </td>
            </tr>
            <tr>
              <td>
                <h1>@ViewBag.Contest.Name</h1>
              </td>
            </tr>
            <tr>
              <td>
                <p>Some winners have not yet had their prize marked as distributed.</p>
              </td>
            </tr>
            <tr>
              <td>
                <p>Do you want to distribute to these winners?</p>
              </td>
            </tr>
            <tr>
              <td>
                  <div class="btn-group btn-group-justified" role="group" id="btn_margin">
                      <div class="btn-group">
                          <input class="btn btn-default" id="btn_yes" value="Yes" onclick="location.href='@Url.Action("Distribute", new { Distribute = true })'" />
                      </div>
                      <div class="btn-group" >
                          <input class=" btn btn-default" id="btn_no" value="No" onclick="location.href='@Url.Action("Distribute", new { Distribute = false })'" />
                          </div>
                      <div class="btn-group">
                          <input class="btn btn-default" id="btn_cancel" value="Cancel" onclick="location.href='@Url.Action("Selection")'"  />
                      </div>
                      </div>
              </td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
